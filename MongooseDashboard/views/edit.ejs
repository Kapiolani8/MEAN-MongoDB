        <body>
            <h3>Friends Dashboard</h3>
            <h4>Edit info about your friends:</h4>
            <form action="/editFriend/<%=friends.id%>" method="post">
                <label>First Name:</label>
                <input type="text" name="firstName" value=""><br>
                <label>Last Name:</label>
                <input type="text" name="lastName" value=""><br>
                <label>Age:</label>
                <input type="text" name="age" value=""><br>
                <input type="submit" value="Update your pal!">
            </form>
            <%= friends.id %>
        </body>
    </html>